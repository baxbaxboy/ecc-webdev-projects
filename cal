<!DOCTYPE html>
<head>
<script>

var event = {
  'summary': 'her house',
  'location': '80 the street',
  'description': 'A chance to get api ',
  'start': {
    'dateTime': '2019-11-08T09:00:00-07:00',
    'timeZone': 'America/New_York'
  },
  'end': {
    'dateTime': '2019-11-08T17:00:00-07:00',
    'timeZone': 'America/New_York'
  },
  'recurrence': [
    'RRULE:FREQ=DAILY;COUNT=2'
  ],
  'attendees': [
    {'email': 'tylermule74@gmail.com'},
    {'email': 'tylermule74@gmail.com'}
  ],
  'reminders': {
    'useDefault': true,
    'overrides': [
      {'method': 'email', 'minutes': 24 * 60},
      {'method': 'popup', 'minutes': 10}
    ]
  }
};

var request = gapi.client.calendar.events.insert({
  'calendarId': 'primary',
  'resource': event
});

request.execute(function(event) {
  appendPre('Event created: ' + event.htmlLink);
});







</script>
</head>
<body>

</body>
</html>
